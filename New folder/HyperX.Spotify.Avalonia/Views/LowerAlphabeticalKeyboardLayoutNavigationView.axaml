<RadioButton
    x:Class="HyperX.Spotify.Avalonia.LowerAlphabeticalKeyboardLayoutNavigationView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Content="abc"
    Focusable="False"
    GroupName="KeyboardLayout"
    IsChecked="True">
    <Interaction.Behaviors>
        <EventTriggerBehavior EventName="Click">
            <NavigateAction Name="LowerAlphabeticalKeyboardLayout" TargetName="KeyboardLayout" />
        </EventTriggerBehavior>
    </Interaction.Behaviors>
</RadioButton>

<SettingsExpander
    x:Class="HyperX.WiFi.Avalonia.WiFiConnectionView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Description="Connection placeholder"
    Header="Connection name"
    IsClickEnabled="True">
    <Interaction.Behaviors>
        <EventTriggerBehavior EventName="Click">
            <NavigateAction
                Name="WiFiConnectionInformation"
                Context="Root"
                Scope="WiFiComponent" />
        </EventTriggerBehavior>
    </Interaction.Behaviors>
</SettingsExpander>
